{
  "pageObject": {
    "className": "GoogleSearchPage",
    "locators": [
      {
        "name": "searchInput",
        "elementType": "input",
        "actions": [
          "fill",
          "clear",
          "getValue"
        ],
        "selectors": [
          {
            "type": "css",
            "value": "[name=\"q\"]"
          }
        ],
        "waitBefore": "visible",
        "validateAfter": true
      },
      {
        "name": "searchButton",
        "elementType": "button",
        "actions": [
          "click"
        ],
        "selectors": [
          {
            "type": "css",
            "value": "[name=\"btnK\"]"
          }
        ],
        "waitBefore": "enabled",
        "validateAfter": true
      }
    ]
  },
  "testSteps": [
    {
      "action": "navigate",
      "params": [
        "https://www.google.com/"
      ],
      "waitFor": {
        "element": "searchInput",
        "state": "visible"
      }
    },
    {
      "action": "fillSearchInput",
      "params": [
        "Playwright"
      ],
      "waitFor": {
        "element": "searchInput",
        "state": "visible"
      }
    },
    {
      "action": "clickSearchButton",
      "params": [],
      "waitFor": {
        "element": "searchButton",
        "state": "enabled"
      },
      "assert": {
        "type": "urlContains",
        "expected": "q=Playwright"
      }
    }
  ]
}