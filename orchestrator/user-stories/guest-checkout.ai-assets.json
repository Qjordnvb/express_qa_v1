{
  "pageObject": {
    "className": "LoginPage",
    "locators": [
      {
        "name": "emailInput",
        "actions": [
          "fill"
        ],
        "selectors": [
          {
            "type": "getByLabel",
            "value": "E-Mail Address"
          },
          {
            "type": "locator",
            "value": "#input-email"
          }
        ]
      },
      {
        "name": "passwordInput",
        "actions": [
          "fill"
        ],
        "selectors": [
          {
            "type": "getByLabel",
            "value": "Password"
          },
          {
            "type": "locator",
            "value": "#input-password"
          }
        ]
      },
      {
        "name": "loginButton",
        "actions": [
          "click"
        ],
        "selectors": [
          {
            "type": "getByRole",
            "value": "button",
            "options": {
              "name": "Login"
            }
          },
          {
            "type": "locator",
            "value": "#button-login"
          }
        ]
      }
    ]
  },
  "testSteps": [
    {
      "action": "navigate",
      "params": [
        "/account/login"
      ]
    },
    {
      "action": "fillEmailInput",
      "params": [
        "test@example.com"
      ]
    },
    {
      "action": "fillPasswordInput",
      "params": [
        "password"
      ]
    },
    {
      "action": "clickLoginButton",
      "params": []
    }
  ]
}